{
  "name": "@folio/data-import",
  "version": "1.0.0",
  "description": "Data Import manager",
  "main": "src/index.js",
  "repository": "folio-org/ui-data-import",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "test": "stripes test karma",
    "test-coverage": "stripes test karma --coverage",
    "lint": "eslint .",
    "lintfix": "eslint . --fix"
  },
  "devDependencies": {
    "@bigtest/interactor": "^0.9.1",
    "@bigtest/mirage": "^0.0.1",
    "@bigtest/mocha": "^0.5.2",
    "@bigtest/react": "^0.1.2",
    "@folio/eslint-config-stripes": "^3.2.1",
    "@folio/stripes": "^1.0.0",
    "@folio/stripes-cli": "^1.7.0",
    "@folio/stripes-core": "^2.17.0",
    "babel-eslint": "^9.0.0",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^5.5.0",
    "mocha": "^5.2.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "sinon": "^7.2.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.16.4",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react-dropzone": "^7.0.1",
    "react-intl": "^2.3.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.1.1"
  },
  "peerDependencies": {
    "@folio/stripes": "^1.0.0",
    "react": "*"
  },
  "stripes": {
    "type": "app",
    "displayName": "ui-data-import.meta.title",
    "route": "/data-import",
    "hasSettings": true,
    "icons": [
      {
        "name": "jobProfiles",
        "alt": "Job Profiles",
        "title": "Job Profiles"
      },
      {
        "name": "matchProfiles",
        "alt": "Match Profiles",
        "title": "Match Profiles"
      },
      {
        "name": "actionProfiles",
        "alt": "Action Profiles",
        "title": "Action Profiles"
      },
      {
        "name": "fieldMapping",
        "alt": "Field Mapping Profiles",
        "title": "Field Mapping Profiles"
      },
      {
        "name": "app",
        "alt": "Data import loader for MARC records",
        "title": "Data import"
      }
    ],
    "okapiInterfaces": {
      "data-import": "1.0",
      "source-record-manager": "1.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.data-import.enabled",
        "displayName": "UI: ui-data-import module is enabled",
        "subPermissions": [
          "data-import.upload.definition.post",
          "data-import.upload.definition.get",
          "data-import.upload.definition.file.delete",
          "data-import.upload.file.post",
          "metadata-provider.log.get",
          "metadata-provider.jobexecution.get"
        ],
        "visible": true
      },
      {
        "permissionName": "settings.data-import.enabled",
        "displayName": "Settings (data-import): display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": true
      }
    ]
  }
}
